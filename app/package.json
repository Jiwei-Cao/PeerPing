{
  "name": "peerly",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "generate-api": "openapi-generator-cli generate -i ../docs/openapi.yaml -g typescript-fetch -o src/api --additional-properties=typescriptThreePlus=true,supportsES6=true,withInterfaces=true",
    "postinstall": "npm run generate-api",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "expo": "~50.0.0",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "react-native-screens": "~3.29.0",
    "react-native-safe-area-context": "4.8.2",
    "@tanstack/react-query": "^5.17.0",
    "zustand": "^4.4.7",
    "react-native-mmkv": "^2.11.0",
    "expo-secure-store": "~12.8.1",
    "react-hook-form": "^7.48.2",
    "react-native-toast-message": "^2.2.0",
    "expo-image-picker": "~14.7.1",
    "expo-notifications": "~0.27.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "@expo/vector-icons": "^14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "@types/react-native": "~0.73.0",
    "typescript": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "@openapitools/openapi-generator-cli": "^2.8.0"
  },
  "private": true
}